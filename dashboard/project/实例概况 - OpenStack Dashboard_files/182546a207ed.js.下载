function setSliderPercent(percent){percent=clamp(percent);var thumbPosition=(percent*100)+'%';var activeTrackPercent=invert?(1-percent)*100+'%':thumbPosition;if(vertical){thumbContainer.css('bottom',thumbPosition);}
